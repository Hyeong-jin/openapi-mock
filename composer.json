{
    "name": "strider2038/swagger-mock",
    "description": "Swagger mock server with fake data generation support",
    "keywords": ["Swagger", "OpenAPI", "OpenAPI3", "Mock", "Server", "Mock-Server"],
    "authors": [
        {
            "name": "Igor Lazarev",
            "email": "strider2038@yandex.ru"
        }
    ],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "7.2.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/collections": "1.*",
        "fzaninotto/faker": "1.*",
        "guzzlehttp/guzzle": "6.*",
        "symfony/cache": "4.*",
        "symfony/console": "4.*",
        "symfony/flex": "1.*",
        "symfony/framework-bundle": "4.*",
        "symfony/monolog-bundle": "3.*",
        "symfony/serializer-pack": "1.*",
        "symfony/yaml": "4.*",
        "willdurand/negotiation": "2.*"
    },
    "require-dev": {
        "behat/behat": "dev-master",
        "behat/mink": "dev-master",
        "behat/mink-browserkit-driver": "1.*",
        "behat/mink-extension": "2.*",
        "behat/symfony2-extension": "2.*",
        "behatch/contexts": "3.*",
        "phake/phake": "3.*",
        "phpunit/phpunit": "7.*",
        "symfony/expression-language": "4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
